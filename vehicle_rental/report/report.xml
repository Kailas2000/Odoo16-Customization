<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Report action to create the PDF, Here report name and file are mention.-->
    <record id="action_report_vehicle_rental" model="ir.actions.report">
        <field name="name">Rental Report</field>
        <field name="model">report.wizard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">vehicle_rental.report_vehicle_rent</field>
        <field name="report_file">vehicle_rental.report_vehicle_rent</field>
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>
    </record>
</odoo>

